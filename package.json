{
  "name": "auth-app",
  "version": "1.0.0",
  "description": "auth-app",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon ./src/index.ts",
    "migration:up": "npx cross-env NODE_OPTIONS=--loader=ts-node/esm migrate-mongo up",
    "migration:down": "npx cross-env NODE_OPTIONS=--loader=ts-node/esm migrate-mongo down",
    "migration:create": "npx migrate-mongo create",
    "migration:reset": "npm run migration:down && npm run migration:up",
    "build": "tsc && tsc-alias",
    "fresh-start": "npm i && npm run migration:reset && npm run build && npm run dev",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [],
  "author": "Digen More",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "http-status": "^2.1.0",
    "ioredis": "^5.9.2",
    "joi": "^18.0.2",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "tsc-alias": "^1.8.16",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/http-status": "^0.2.30",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^25.2.0",
    "@types/nodemailer": "^7.0.9",
    "@types/nodemon": "^3.1.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/typescript": "^2.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "migrate-mongo": "^14.0.7",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
